{
  "name": "instagram-stalk",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "concurrently -k -r \"npm run start:client\" \"npm run start:functions\" \"npm run start:serve\"",
    "start:client": "cd ./client && npm run build:dev",
    "start:functions": "cd ./functions && npm run compile:dev",
    "start:serve": "firebase serve --only hosting,functions",
    "postinstall": "concurrently -k \"cd ./client && npm install\" \"cd ./functions && npm install\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^3.5.0"
  }
}
